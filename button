#include <Arduino.h>

int i = 0;

void setup() {
  pinMode(LED_BUILTIN, OUTPUT);
  pinMode(2, OUTPUT);
  Serial.begin(9600);
}

void loop() {
  if (digitalRead(2) == HIGH) {
  digitalWrite(LED_BUILTIN, HIGH);
  Serial.print("KONTOOOOLLLLLL ");
  Serial.println(i);
  i++;
  } else {
  digitalWrite(LED_BUILTIN, LOW);
  }
}
